{
  "compilerOptions": {
    "paths": {
      "#core/*": [
        "./src/core/*.js"
      ],
      "#resources": [
        "./src/resources.js"
      ],
      "#models": [
        "./src/models.js"
      ],
      "#resources/*": [
        "./src/resources/*.js"
      ],
      "#models/*": [
        "./src/models/*.js"
      ],
      "#config": [
        "./src/config.js"
      ],
      "#lib/*": [
        "./src/lib/.*.js"
      ],
      "#services": [
        "./src/services.js"
      ]
    },
  },
  "extends": "../tsconfig.base.json",
  "references": [
    {
      "path": "./tsconfig.src.json"
    },
    {
      "path": "./tsconfig.test.json"
    }
  ],
}