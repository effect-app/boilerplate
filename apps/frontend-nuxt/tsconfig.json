{
  // https://v3.nuxtjs.org/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "tsPlusConfig": "../../tsplus.config.json",
    "lib": [
      "DOM",
      "dom.iterable",
      "ES2022"
    ],
    //"moduleResolution": "Node16",
    "strict": true,
    "paths": {
      "@effect-ts-app/boilerplate-vue": [
        "../../packages/vue/dist"
      ],
      "@effect-ts-app/boilerplate-vue/*": [
        "../../packages/vue/dist/*"
      ],
      "@effect-ts-app/boilerplate-client": [
        "../../packages/client/dist"
      ],
      "@effect-ts-app/boilerplate-client/*": [
        "../../packages/client/dist/*"
      ],
      "@effect-ts-app/boilerplate-types": [
        "../../packages/types/dist"
      ],
      "@effect-ts-app/boilerplate-types/*": [
        "../../packages/types/dist/*"
      ],
      "@effect-ts-app/boilerplate-prelude": [
        "../../packages/prelude/dist"
      ],
      "@effect-ts-app/boilerplate-prelude/*": [
        "../../packages/prelude/dist/*"
      ],
      // copied from ./.nuxt/tsconfig.json
      "~~": [
        "."
      ],
      "~~/*": [
        "./*"
      ],
      "@@": [
        "."
      ],
      "@@/*": [
        "./*"
      ],
      "~": [
        "."
      ],
      "~/*": [
        "./*"
      ],
      "@": [
        "."
      ],
      "@/*": [
        "./*"
      ],
      "assets": [
        "assets"
      ],
      "public": [
        "public"
      ],
      "#app": [
        "../../node_modules/nuxt/dist/app"
      ],
      "#app/*": [
        "../../node_modules/nuxt/dist/app/*"
      ],
      "vue-demi": [
        "../../node_modules/nuxt/dist/app/compat/vue-demi"
      ],
      "#head": [
        "../../node_modules/nuxt/dist/head/runtime"
      ],
      "#head/*": [
        "../../node_modules/nuxt/dist/head/runtime/*"
      ],
      "#components": [
        ".nuxt/components"
      ],
      "#imports": [
        ".nuxt/imports"
      ],
      "#build": [
        ".nuxt"
      ],
      "#build/*": [
        ".nuxt/*"
      ]
    },
    "types": [
      "node",
      "../../types/global",
      "../../types/modules"
    ],
  },
}