    "npm-check-updates": "^16.14.20",
    "playwright-core": "^1.43.1",
    "playwright-watch": "^1.3.23",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0"
  },
  "dependencies": {
    "@effect-app-boilerplate/api": "workspace:*",
    "@effect/platform-node": "0.48.3",
    "effect-app": "^1.2.3",
    "@effect-app/core": "^1.1.3",
    "@effect-app/schema": "^1.1.3",
