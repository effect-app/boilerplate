{
  "compilerOptions": {
    "plugins": [
      {
        "name": "ts-plugin-sort-import-suggestions",
        "moveUpPatterns": [
          "\\.{1,2}/",
          "^(?:\\.\\./)+",
          "^#",
          "^@/",
          "effect-app",
          "^@effect-app/",
          "effect",
          "^@effect/"
        ],
        "moveDownPatterns": [
          "^node_modules/"
        ],
        "overrides": {
          "effect-app": [
            "Array",
            "Option",
            "Either"
          ]
        }
      },
      {
        "name": "@effect/language-service"
      }
    ]
  }
}