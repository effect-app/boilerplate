        "@types/http-proxy": "^1.17.15",
        "@types/markdown-it": "^14.1.2",
        "eslint-plugin-vue": "^9.32.0",
        "h3": "^1.13.0",
        "nuxt": "~3.15.1",
        "sass": "^1.83.1",
        "typescript": "~5.7.2",
        "vue-tsc": "^2.2.0"
