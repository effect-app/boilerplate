        "@vueuse/core": "^12.3.0",
        "@vueuse/nuxt": "^12.3.0",
        "cookie": "^1.0.2",
        "date-fns": "^4.1.0",
        "effect": "^3.12.1",
        "effect-app": "^2.23.2",
        "highcharts": "^12.1.2",
        "http-proxy-node16": "^1.0.5",
        "mitt": "^3.0.1",
        "papaparse": "^5.4.1",
        "reconnecting-eventsource": "^1.6.3",
