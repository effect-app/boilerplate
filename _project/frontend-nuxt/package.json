{
  "name": "@effect-app-boilerplate/frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "compile": "nuxt prepare && effect-app-cli watch vue-tsc --noEmit",
    "watch": "([ ! -d './.nuxt' ] && nuxt prepare || echo 'no prepare') && effect-app-cli watch vue-tsc --noEmit -w",
    "dev": "PORT=4000 nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "ncu": "ncu",
    "lint": "NODE_OPTIONS=--max-old-space-size=8192 ESLINT_TS=1 eslint --ext .ts,.vue --ignore-path ../../.gitignore .",
    "autofix": "pnpm lint --fix",
    "testsuite": "pnpm lint",
    "clean": "rm -rf ./.nuxt ./node_modules/.vite"
  },
  "dependencies": {
    "@effect-app/prelude": "^0.48.0",
    "@effect-app/vue": "^0.41.0",
    "@effect-app/core": "^0.49.0",
    "@effect-app/schema": "^0.43.0",
    "@effect/io": "^0.25.2",
    "@effect/data": "^0.12.2",
    "@effect-app-boilerplate/resources": "*",
    "@effect-app-boilerplate/models": "*",
    "@vueuse/core": "^10.1.0",
    "@vueuse/nuxt": "^10.1.0",
    "cookie": "^0.5.0",
    "date-fns": "^2.29.3",
    "http-proxy": "npm:http-proxy-node16@^1.0.2",
    "mitt": "^3.0.0",
    "reconnecting-eventsource": "^1.6.2",
    "vue-timeago3": "^2.3.0",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.1.15"
  },
  "devDependencies": {
    "@mdi/js": "^7.2.96",
    "@types/cookie": "^0.5.1",
    "@types/http-proxy": "^1.17.10",
    "eslint-plugin-vue": "^9.11.0",
    "h3": "^1.6.4",
    "nuxt": "~3.4.2",
    "sass": "^1.62.0",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.176/compiler/typescript.tgz",
    "vue-tsc": "~1.2.0"
  },
  "resolutions": {
    "http-proxy": "npm:http-proxy-node16@1.0.0"
  }
}