{
  "name": "@effect-app-boilerplate/messages",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "peerDependencies": {},
  "dependencies": {
    "@effect-app/core": "^0.11.3",
    "@effect-app/infra-adapters": "^0.11.3",
    "@effect-app/schema": "^0.11.3",
    "@effect-app/infra": "^0.12.5",
    "@effect-app/prelude": "^0.11.5",
    "@effect/io": "^0.0.66",
    "@fp-ts/core": "^0.0.11",
    "@fp-ts/data": "^0.0.40",
    "@effect-app-boilerplate/resources": "*",
    "@effect-app-boilerplate/types": "*",
    "change-case": "^4.1.2",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "path-parser": "^6.1.0"
  },
  "devDependencies": {},
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./_cjs/index.cjs"
      }
    },
    "./Messages/PrintQueue/PrintOnePick": {
      "import": {
        "types": "./dist/Messages/PrintQueue/PrintOnePick.d.ts",
        "default": "./dist/Messages/PrintQueue/PrintOnePick.js"
      },
      "require": {
        "types": "./dist/Messages/PrintQueue/PrintOnePick.d.ts",
        "default": "./_cjs/Messages/PrintQueue/PrintOnePick.cjs"
      }
    },
    "./basicRuntime": {
      "import": {
        "types": "./dist/basicRuntime.d.ts",
        "default": "./dist/basicRuntime.js"
      },
      "require": {
        "types": "./dist/basicRuntime.d.ts",
        "default": "./_cjs/basicRuntime.cjs"
      }
    },
    "./RequestLayers": {
      "import": {
        "types": "./dist/RequestLayers.d.ts",
        "default": "./dist/RequestLayers.js"
      },
      "require": {
        "types": "./dist/RequestLayers.d.ts",
        "default": "./_cjs/RequestLayers.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      }
    }
  },
  "scripts": {
    "build": "pnpm clean-dist && NODE_OPTIONS=--max-old-space-size=6144 tsc --build",
    "build2:cjs": "rm -rf _cjs && babel dist --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .cjs --source-maps",
    "watch": "pnpm build --watch",
    "watch2": "pnpm clean-dist && NODE_OPTIONS=--max-old-space-size=6144 tsc -w",
    "clean": "rm -rf dist && rm -rf _cjs",
    "clean-dist": "sh ../../scripts/clean-dist.sh",
    "circular": "pnpm circular:src && pnpm circular:dist",
    "circular:src": "madge --circular --ts-config ./tsconfig.json --extensions ts ./_src",
    "circular:dist": "madge --circular --extensions js ./dist",
    "compile": "NODE_OPTIONS=--max-old-space-size=6144 tsc --noEmit",
    "lint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint ./_src",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx .",
    "autofix": "pnpm lint --fix",
    "test": "vitest",
    "test:run": "pnpm run test run --passWithNoTests",
    "testsuite": "pnpm lint && pnpm circular && pnpm run test:run",
    "ncu": "ncu"
  },
  "gitHead": "bd8e27eea3eff97db8739d577d67e7336c078d28",
  "sideEffects": false
}