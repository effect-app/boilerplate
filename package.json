{
  "name": "@effect-app-boilerplate/root",
  "private": true,
  "type": "module",
  "pnpm": {
    "patchedDependencies": {
      "@effect/language-service@0.0.19": "patches/@effect__language-service@0.0.19.patch",
      "@typescript-eslint/scope-manager@6.16.0": "patches/@typescript-eslint__scope-manager@6.16.0.patch",
      "eslint-plugin-codegen@0.17.0": "patches/eslint-plugin-codegen@0.17.0.patch",
      "@phaphoso/eslint-plugin-dprint@0.5.2": "patches/@phaphoso__eslint-plugin-dprint@0.5.2.patch",
      "date-fns@2.30.0": "patches/date-fns@2.30.0.patch",
      "fast-check@3.15.0": "patches/fast-check@3.15.0.patch",
      "@fp-ts/optic@0.21.0": "patches/@fp-ts__optic@0.21.0.patch"
    }
  },
  "scripts": {
    "all": "pnpm -r --filter \"!./play\" --filter \"!./libs/**/*\"",
    "preinstall": "npx only-allow pnpm",
    "clean": "pnpm all clean",
    "clean-dist": "pnpm all clean-dist",
    "upgrade": "echo hi",
    "scan": "pnpm esw -w --fix _project/api/_src/Usecases.ts _project/resources/_src/Resources.ts _project/resources/_src/Resources/*.ts",
    "autofix": "NODE_OPTIONS=--max-old-space-size=8192 pnpm eslint _project --fix && pnpm lint-frontend --fix && pnpm lint-e2e --fix",
    "lint": "pnpm eslint _project && pnpm lint-frontend && pnpm lint-e2e",
    "circular:dist": "pnpm all circular:dist",
    "start": "concurrently -k -p \"[{name}]\" -n \"API,Frontend\" -c \"cyan.bold,green.bold,blue.bold\" \"pnpm start:api\" \"pnpm start:frontend\"",
    "start:api": "cd _project/api && pnpm start",
    "start:frontend": "cd _project/frontend && pnpm start",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Watch,API,Frontend\" -c \"yellow.bold,cyan.bold,green.bold\" \"pnpm watch\" \"pnpm dev:api\" \"pnpm dev:frontend\"",
    "online-dev": "cd _project/frontend && pnpm online-dev",
    "dev:api": "cd _project/api && pnpm dev",
    "dev:frontend": "cd _project/frontend && pnpm dev",
    "build-apps": "cd _project/api && pnpm build && cd ../frontend && pnpm build",
    "test": "pnpm all test",
    "testsuite": "pnpm all testsuite",
    "up-all": "pnpm --recursive update",
    "up-ncu": "pnpm run update && pnpm i",
    "update": "ncu -u --target @latest && pnpm -r ncu -u --target @latest",
    "ncu": "ncu",
    "build-libs": "cd libs && pnpm build && cd ../.. && pnpm build",
    "l": "./scripts/link.sh",
    "ul": "./scripts/unlink.sh",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx",
    "apps": "pnpm all --workspace-concurrency 1 --filter !./_project/messages --filter !./_project/resources --filter !./_project/models --filter !./_project/ui",
    "api": "pnpm all --workspace-concurrency 1 --filter \"./_project/api\"",
    "packages": "pnpm all --workspace-concurrency 1 --filter ./_project/resources --filter ./_project/models --filter ./_project/ui --filter \"./_project/messages\"",
    "libs": "pnpm all --workspace-concurrency 1 --filter \"@effect-app/*\"",
    "circular-api": "pnpm api circular",
    "circular-packages": "pnpm packages circular",
    "lint-api": "pnpm eslint -c ./.eslintrc.api.cjs _project/api",
    "lint-e2e": "cd e2e && pnpm lint",
    "lint-frontend": "cd _project/frontend && pnpm lint",
    "lint-packages": "pnpm eslint -c ./.eslintrc.packages.cjs _project/models _project/resources _project/messages",
    "test-packages": "pnpm packages test:run",
    "testsuite-packages-manual": "cd _project/models && pnpm testsuite && cd ../resources && pnpm testsuite && cd ../messages && pnpm testsuite",
    "watch-packages": "pnpm packages build && pnpm packages watch",
    "build:tsc": "NODE_OPTIONS=--max-old-space-size=8192 effect-app-cli packagejson-target \"./_project/models\" effect-app-cli index-multi tsc --build",
    "build": "pnpm build:tsc ./tsconfig.all.json && cd _project/frontend && pnpm compile",
    "watch": "pnpm build:tsc ./tsconfig.all.json --watch",
    "watch-src": "pnpm build:tsc ./tsconfig.src.json --watch",
    "build-prod": "pnpm packages build && pnpm build-frontend-prod",
    "build-frontend-prod": "cd _project/frontend && pnpm build",
    "rebuild": "pnpm all clean && pnpm -r build",
    "nnm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && pnpm i"
  },
  "resolutions": {
    "@effect/schema": "npm:@effect-app/schema-fork@0.0.0-20231228152439",
    "date-fns": "^2.29.3",
    "http-proxy": "npm:http-proxy-node16@1.0.0",
    "vue": "^3.3.9"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7",
    "@opentelemetry/exporter-trace-otlp-http": "^0.46.0",
    "@opentelemetry/resources": "^1.19",
    "@opentelemetry/sdk-metrics": "^1.19",
    "@opentelemetry/sdk-trace-base": "^1.19",
    "@opentelemetry/sdk-trace-node": "^1.19",
    "@opentelemetry/sdk-trace-web": "^1.19",
    "@opentelemetry/semantic-conventions": "^1.19",
    "cross-env": "^7.0.3",
    "fast-check": "^3.15.0",
    "patch-package": "^8.0.0",
    "ts-node": "^10.9.2",
    "vite-node": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@dprint/typescript": "^0.88.7",
    "@effect-app/cli": "^0.33.1",
    "@effect-app/eslint-codegen-model": "^0.36.0-next.1",
    "@effect-app/prelude": "^0.107.0-next.30",
    "@effect-app/core": "^0.97.0-next.4",
    "@effect-app/compiler": "^0.48.0-next.0",
    "@effect/language-service": "^0.0.19",
    "@phaphoso/eslint-plugin-dprint": "^0.5.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "~20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@typescript-eslint/scope-manager": "^6.16.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "concurrently": "^8.2.2",
    "dprint": "^0.45.0",
    "enhanced-resolve": "^5.15.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-codegen": "^0.17.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-watch": "^8.0.0",
    "madge": "^6.1.0",
    "nodemon": "^3.0.2",
    "npm-check-updates": "^16.14.12",
    "prebuild-install": "^7.1.1",
    "ts-toolbelt": "^9.6.0",
    "ts-transform-paths": "^3.0.0",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "npm:@effect-app/typescript@5.4.0-tsplus.202311222",
    "typescript-transform-paths": "^3.4.6",
    "unplugin-auto-import": "^0.17.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}