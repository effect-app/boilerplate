{
    "name": "@effect-app-boilerplate/root",
    "private": true,
    "pnpm": {
        "patchedDependencies": {
            "@effect/language-service@0.0.19": "patches/@effect__language-service@0.0.19.patch",
            "@playwright/test@1.30.0": "patches/@playwright+test+1.30.0.patch",
            "@typescript-eslint/scope-manager@5.50.0": "patches/@typescript-eslint__scope-manager@5.50.0.patch",
            "eslint-plugin-codegen@0.17.0": "patches/eslint-plugin-codegen@0.17.0.patch",
            "eslint-plugin-unused-imports@2.0.0": "patches/eslint-plugin-unused-imports@2.0.0.patch",
            "typescript@5.0.0-tsplus.20230205": "patches/typescript@5.0.0-tsplus.20230205.patch",
            "@phaphoso/eslint-plugin-dprint@0.5.2": "patches/@phaphoso__eslint-plugin-dprint@0.5.2.patch",
            "date-fns@2.29.3": "patches/date-fns@2.29.3.patch"
        }
    },
    "scripts": {
        "all": "pnpm -r --filter \"!./play\" --filter \"!./libs/**/*\"",
        "preinstall": "npx only-allow pnpm",
        "clean": "pnpm all clean",
        "clean-dist": "pnpm all clean-dist",
        "upgrade": "echo hi",
        "scan": "pnpm esw -w --fix _project/api/_src/Usecases.ts _project/resources/_src/Resources.ts _project/resources/_src/Resources/*.ts",
        "autofix": "NODE_OPTIONS=--max-old-space-size=6144 pnpm all autofix",
        "lint": "pnpm all lint",
        "circular:dist": "pnpm all circular:dist",
        "start": "concurrently -k -p \"[{name}]\" -n \"API,Frontend\" -c \"cyan.bold,green.bold,blue.bold\" \"pnpm start:api\" \"pnpm start:frontend\"",
        "start:api": "cd _project/main && pnpm start",
        "start:frontend": "cd _project/frontend-nuxt && pnpm start",
        "dev": "concurrently -k -p \"[{name}]\" -n \"Watch,API,Frontend\" -c \"yellow.bold,cyan.bold,green.bold\" \"pnpm watch\" \"pnpm dev:api\" \"pnpm dev:frontend\"",
        "online-dev": "cd _project/frontend-nuxt && pnpm online-dev",
        "dev:api": "cd _project/main && pnpm dev",
        "dev:frontend": "cd _project/frontend-nuxt && pnpm dev",
        "build-apps": "cd _project/main && pnpm build && cd ../frontend && pnpm build",
        "test": "pnpm all test",
        "testsuite": "pnpm all testsuite",
        "up-all": "pnpm --recursive update",
        "up-ncu": "pnpm run update && pnpm i",
        "update": "ncu -u && pnpm -r ncu -u",
        "ncu": "ncu",
        "build-libs": "cd libs && cd _project/core && pnpm build && cd ../.. && pnpm build",
        "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx",
        "apps": "pnpm all --workspace-concurrency 1 --filter !./_project/messages --filter !./_project/resources --filter !./_project/models --filter !./_project/ui",
        "api": "pnpm all --workspace-concurrency 1 --filter \"./_project/main*\"",
        "packages": "pnpm all --workspace-concurrency 1 --filter ./_project/messages --filter ./_project/resources --filter ./_project/models --filter ./_project/ui",
        "libs": "pnpm all --workspace-concurrency 1 --filter \"@effect-app/*\"",
        "circular-api": "pnpm api circular",
        "circular-packages": "pnpm packages circular",
        "lint-api": "pnpm api lint",
        "lint-packages": "pnpm packages lint",
        "test-packages": "pnpm packages test:run",
        "testsuite-packages-manual": "cd _project/models && pnpm testsuite && cd ../resources && pnpm testsuite && cd ../messages && pnpm testsuite",
        "watch-packages": "pnpm packages build && pnpm packages watch",
        "build": "NODE_OPTIONS=--max-old-space-size=6144 effect-app-cli packagejson-target \"./_project/models\" effect-app-cli index-multi tsc --build ./tsconfig.all.json",
        "build-prod": "pnpm packages build && pnpm build-frontend-prod",
        "build-frontend-prod": "cd _project/frontend-nuxt && pnpm build",
        "rebuild": "pnpm all clean && pnpm -r build",
        "nnm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && pnpm i"
    },
    "resolutions": {
        "@types/node": "~18.11.11",
        "@types/react": "^18.0.26",
        "date-fns": "^2.29.3",
        "http-proxy": "npm:http-proxy-node16@1.0.0"
    },
    "dependencies": {
        "@types/react": "^18.0.27",
        "cross-env": "^7.0.3",
        "fast-check": "^3.6.2",
        "patch-package": "^6.5.1",
        "ts-node": "^10.9.1",
        "vite-node": "^0.28.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@dprint/typescript": "^0.82.0",
        "@effect-app/cli": "^0.2.1",
        "@effect-app/eslint-codegen-model": "^0.9.1",
        "@effect-app/prelude": "^0.18.2",
        "@effect-app/core": "^0.19.0",
        "@effect-app/compiler": "^0.10.4",
        "@effect/language-service": "^0.0.19",
        "@phaphoso/eslint-plugin-dprint": "^0.5.2",
        "@tsconfig/strictest": "^1.0.2",
        "@tsplus/installer": "0.0.170",
        "@types/lodash": "^4.14.191",
        "@types/node": "~18.11.19",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "@typescript-eslint/scope-manager": "^5.50.0",
        "babel-plugin-replace-import-extension": "^1.1.3",
        "concurrently": "^7.6.0",
        "dprint": "^0.34.4",
        "enhanced-resolve": "^5.12.0",
        "eslint": "^8.33.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-codegen": "^0.17.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier-vue": "^4.2.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sort-destructure-keys": "^1.4.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "eslint-watch": "^8.0.0",
        "madge": "^6.0.0",
        "nodemon": "^2.0.20",
        "npm-check-updates": "^16.6.4",
        "prebuild-install": "^7.1.1",
        "ts-toolbelt": "^9.6.0",
        "ts-transform-paths": "^3.0.0",
        "tsc-watch": "^6.0.0",
        "tsconfig-paths": "^4.1.2",
        "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.170/compiler/typescript.tgz",
        "typescript-transform-paths": "^3.4.6",
        "unplugin-auto-import": "^0.13.0",
        "vite": "^4.1.1",
        "vitest": "^0.28.4"
    }
}